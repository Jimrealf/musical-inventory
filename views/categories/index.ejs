<!DOCTYPE html>
<html>
<head>
    <title>Musical Inventory</title>
    <link rel="stylesheet" href="/styles.css">
</head>
<body>
    <h1>Musical Inventory</h1>
    <a href="/categories/new">Add Category</a>
    <ul>
    <% categories.forEach(category => { %>
        <li>
            <a href="/categories/<%= category.id %>"><%= category.name %></a>
        </li>
    <% }) %>
    </ul>
</body>
</html>